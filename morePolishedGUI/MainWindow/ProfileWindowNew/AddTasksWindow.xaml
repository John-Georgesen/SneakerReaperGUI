        <SolidColorBrush x:Key="ComboBox.MouseOver.Editable.Border" Color="#101017"/>

        <SolidColorBrush x:Key="ComboBox.MouseOver.Editable.Button.Background" Color="#101017"/>
        <SolidColorBrush x:Key="ComboBox.MouseOver.Editable.Button.Border" Color="#101017"/>
        <SolidColorBrush x:Key="ComboBox.Pressed.Glyph" Color="#FF000000"/>
